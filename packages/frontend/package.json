{
    "homepage": "https://wallet.near.org",
    "name": "@near-wallet/frontend",
    "version": "0.2.0",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "pre-deploy": "yarn build",
        "update:static": "npx copyfiles 'src/wasm/*.wasm' 'dist'",
        "start": "yarn update:static && parcel src/index.html --cache-dir ./.parcel-cache --https --cert devServerCertificates/primary.crt --key devServerCertificates/private.pem",
        "pre-build": "npx rimraf dist/ && npx rimraf .parcel-cache/",
        "build": "yarn run bundle && yarn run sentry",
        "bundle": "cross-env NODE_ENV=production node ci/runBundler.js",
        "check": "tsc --noEmit",
        "dev": "yarn && cross-env NEAR_WALLET_ENV=mainnet yarn start",
        "sentry": "node ci/sentry-send-release.js",
        "test": "jest",
        "lint": "yarn rome check .",
        "prepush": "concurrently \"yarn run test\" \"yarn lint\"",
        "precommit": "node .githooks/format-json-script.mjs",
        "preinstall": "npx only-allow yarn"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "dependencies": {
        "@ledgerhq/hw-transport-u2f": "^5.34.0",
        "@near-wallet-selector/account-export": "^7.6.1",
        "@reduxjs/toolkit": "1.6.2",
        "@sentry/browser": "^6.4.1",
        "big.js": "^6.2.1",
        "bip39-light": "^1.0.7",
        "connected-react-router": "^6.6.0",
        "dataloader": "^2.1.0",
        "escape-html": "^1.0.3",
        "fetch-send-json": "0.0.2",
        "i18next": "^22.0.4",
        "js-sha256": "^0.9.0",
        "lodash.assign": "^4.2.0",
        "lodash.clonedeep": "^4.5.0",
        "lodash.isarray": "^4.0.0",
        "lodash.isequal": "^4.5.0",
        "lodash.isplainobject": "^4.0.6",
        "lodash.isstring": "^4.0.1",
        "lodash.mapvalues": "^4.6.0",
        "lodash.merge": "^4.6.2",
        "lodash.mergewith": "^4.6.2",
        "lodash.omit": "^4.5.0",
        "lodash.once": "^4.1.1",
        "lodash.set": "^4.3.2",
        "lodash.throttle": "^4.1.1",
        "lodash.unset": "^4.5.2",
        "lodash.update": "^4.10.2",
        "mixpanel-browser": "^2.41.0",
        "near-api-js": "^0.45.1",
        "near-ledger-js": "^0.2.1",
        "near-seed-phrase": "^0.2.0",
        "node-cache": "^5.1.2",
        "prop-types": "^15.7.2",
        "qrcode.react": "^1.0.1",
        "query-string": "^7.0.0",
        "react": "^16.13.1",
        "react-countdown-circle-timer": "^3.0.9",
        "react-dom": "^16.13.1",
        "react-google-recaptcha": "^2.1.0",
        "react-google-recaptcha-v3-near": "^2.0.0",
        "react-helmet": "^6.1.0",
        "react-i18next": "^12.0.0",
        "react-localize-redux": "^3.5.3",
        "react-phone-number-input": "2.3.11",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.2.0",
        "react-textfit": "^1.1.1",
        "reduce-reducers": "^1.0.4",
        "redux-actions": "^2.6.5",
        "redux-thunk": "^2.3.0",
        "regenerator-runtime": "^0.13.5",
        "reselect": "^4.0.0",
        "styled-components": "^5.3.0",
        "timeago.js": "^4.0.2",
        "use-immer": "^0.7.0"
    },
    "devDependencies": {
        "@parcel/config-default": "^2.8.3",
        "@parcel/transformer-raw": "^2.8.3",
        "@parcel/transformer-typescript-tsc": "^2.8.3",
        "@parcel/validator-typescript": "^2.8.3",
        "@types/jest": "^29.5.0",
        "@types/react-router": "^5.1.19",
        "@types/styled-components": "^5.1.26",
        "concurrently": "^7.0.0",
        "cross-env": "^7.0.3",
        "dotenv": "^11.0.0",
        "gleap": "^10.0.0",
        "glob": "^9.2.1",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "jest-svg-transformer": "^1.0.0",
        "parcel": "^2.8.3",
        "sort-json": "^2.0.1",
        "ts-jest": "^29.1.0"
    }
}
