{
    "homepage": "https://wallet.near.org",
    "name": "@near-wallet/frontend",
    "version": "0.2.0",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "pre-deploy": "yarn build",
        "update:static": "npx copyfiles 'src/wasm/*.wasm' 'dist'",
        "start": "yarn update:static && parcel src/index.html --cache-dir ./.parcel-cache --https --cert devServerCertificates/primary.crt --key devServerCertificates/private.pem",
        "start:mainnet": "yarn update:static && parcel src/index.html --port 3000 --cache-dir ./.parcel-cache --https --cert devServerCertificates/primary.crt --key devServerCertificates/private.pem",
        "pre-build": "npx rimraf dist/ && npx rimraf .parcel-cache/",
        "build": "yarn run bundle && yarn run sentry",
        "bundle": "cross-env NODE_ENV=production node ci/runBundler.js",
        "check": "tsc --noEmit",
        "dev": "yarn && cross-env NEAR_WALLET_ENV=testnet REACT_APP_USE_TESTINGLOCKUP=true yarn start",
        "dev:mainnet": "yarn && cross-env NEAR_WALLET_ENV=mainnet REACT_APP_USE_TESTINGLOCKUP=true yarn start:mainnet",
        "sentry": "node ci/sentry-send-release.js",
        "test": "jest",
        "fix": "eslint --ext .js --ext .jsx . --fix",
        "storybook": "storybook dev -p 6006",
        "storybook:build": "storybook build",
        "prepush": "concurrently \"yarn run test\"",
        "precommit": "node .githooks/format-json-script.mjs",
        "preinstall": "npx only-allow yarn"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.4.2",
