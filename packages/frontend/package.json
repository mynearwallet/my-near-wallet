{
  "name": "@near-wallet/frontend",
  "version": "0.2.0",
  "homepage": "https://wallet.near.org",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn run bundle && yarn run sentry",
    "build-storybook": "storybook build",
    "bundle": "cross-env NODE_ENV=production node ci/runBundler.js",
    "check": "tsc --noEmit",
    "dev": "yarn && cross-env NEAR_WALLET_ENV=testnet yarn start",
    "preinstall": "npx only-allow yarn",
    "lint": "yarn rome check .",
    "pre-build": "npx rimraf dist/ && npx rimraf .parcel-cache/",
    "pre-deploy": "yarn build",
    "precommit": "node .githooks/format-json-script.mjs",
    "prepush": "concurrently \"yarn run test\" \"yarn lint\"",
    "sentry": "node ci/sentry-send-release.js",
    "start": "yarn update:static && parcel src/index.html --cache-dir ./.parcel-cache --https --cert devServerCertificates/primary.crt --key devServerCertificates/private.pem",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "update:static": "npx copyfiles 'src/wasm/*.wasm' 'dist'"
  },
  "browserslist": [">0.2%", "not dead", "not ie <= 11", "not op_mini all"],
  "dependencies": {
    "@ledgerhq/hw-transport-u2f": "^5.34.0",
